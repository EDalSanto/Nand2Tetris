/**
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

/**
 * (A AND !sel) OR
 * (B AND sel)
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notSel);
    And(a=a, b=notSel, out=w1);
    And(a=b, b=sel, out=w2);
    Or(a=w1, b=w2, out=out);
}
